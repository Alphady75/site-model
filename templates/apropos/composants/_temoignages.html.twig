							<span class="designation">{{temoignage.fonction}}</span>
						</div>
						<div class="text">{{temoignage.commentaire}}</div>
						<div class="rating">
							<i class="fa fa-star"></i>
							<i class="fa fa-star"></i>
							<i class="fa fa-star"></i>
							<i class="fa fa-star"></i>
							<i class="fa fa-star"></i>
							<span>5.0</span>
						</div>
					</div>
				</div>
			{% endfor %}
		</div>
	</div>
