<!DOCTYPE html>
<html lang="fr">

<head>
    <meta charset="utf-8" />
    <title>{% block title %}{{ appName }}{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="keywords" />
    <meta content="" name="description" />

    <!-- Favicon -->
    <link href="{{ asset('img/favicon.png') }}" rel="icon" />

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&family=Poppins:wght@600;700&display=swap"
        rel="stylesheet" />

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet" />

    <!-- Libraries Stylesheet -->
    <link href="{{ asset('themes/base/lib/animate/animate.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('themes/base/lib/owlcarousel/assets/owl.carousel.min.css') }}" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="{{ asset('themes/base/css/bootstrap.min.css') }}" rel="stylesheet" />

    <!-- Template Stylesheet -->
    <link href="{{ asset('themes/base/css/lightbox.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('themes/base/css/style.css') }}" rel="stylesheet" />

    <!-- Lightboxe -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/venobox/2.0.4/venobox.min.css"
        integrity="sha512-HFaR9dTfvVVIkca85XvaYOlbZqtyRp5f7cyfb3ycnQU60RM1qjmJKq7qZPLDI+nudOkFDuY5giiwQqfbP7M36g=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_link_tags('custom') }}
        {{ encore_entry_link_tags('responsive') }}
    {% endblock %}

    <style>
        .footer h4{
            font-size: 1.3em!important;
        }
        .footer a, .footer p{
            font-size: .9em!important;
        }
        textarea{
            height: 200px!important;
        }
        .radius{
            border-radius: 10px!important;
        }
    </style>
</head>

<body class="{% block class %}{% endblock %}">

    {% block header %}

        {% include "partials/base/_header.html.twig" %}
        
    {% endblock %}

    {% include "partials/_flash.html.twig" %}

    {% block body %}{% endblock %}

    {% block footer %}

        {% include "partials/base/_footer.html.twig" %}

    {% endblock %}

    <!-- JavaScript Libraries -->
    <script src="{{ asset('jquery/jquery-3.4.1.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.bundle.min.js') }}"></script>
    <script src="{{ asset('themes/base/lib/wow/wow.min.js') }}"></script>
    <script src="{{ asset('themes/base/lib/easing/easing.min.js') }}"></script>
    <script src="{{ asset('themes/base/lib/waypoints/waypoints.min.js') }}"></script>
    <script src="{{ asset('themes/base/lib/owlcarousel/owl.carousel.min.js') }}"></script>

    <!-- Template Javascript -->
    <script src="{{ asset('themes/base/js/lightbox.min.js') }}"></script>
    <script src="{{ asset('themes/base/js/main.js') }}"></script>

    {% block javascripts %}
    {{ encore_entry_script_tags('app') }}
    {% endblock %}

    <script src="https://cdnjs.cloudflare.com/ajax/libs/venobox/2.0.4/venobox.min.js"
        integrity="sha512-KX9LF4BMXOG6qr9aGjFIPK1xysZAHWXpuZW6gnRi6oM+41qa8x4zaLPkckNxz5veoSWzmV5HZqPMMtknU+431g=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script>
        new VenoBox({
            selector: '.venobox',
            infinigall: true,
            navigation: true,
            spinner: 'wave'
        });
    </script>
</body>

</html>