{% extends 'base.html.twig' %}

{% block title %}Statut
{% endblock %}

{% block body %}
	<h1>Statut</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ statut.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ statut.name }}</td>
			</tr>
			<tr>
				<th>Created</th>
				<td>{{ statut.created ? statut.created|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>Updated</th>
				<td>{{ statut.updated ? statut.updated|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('statut_index') }}">back to list</a>

	<a href="{{ path('statut_edit', {'id': statut.id}) }}">edit</a>

	{{ include('admin/statut/_delete_form.html.twig') }}
{% endblock %}
