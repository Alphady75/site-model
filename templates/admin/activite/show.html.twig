{% extends 'base.html.twig' %}

{% block title %}Activite
{% endblock %}

{% block body %}
	<h1>Activite</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ activite.id }}</td>
			</tr>
			<tr>
				<th>Icon</th>
				<td>{{ activite.icon }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ activite.name }}</td>
			</tr>
			<tr>
				<th>Slug</th>
				<td>{{ activite.slug }}</td>
			</tr>
			<tr>
				<th>Created</th>
				<td>{{ activite.created ? activite.created|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>Updated</th>
				<td>{{ activite.updated ? activite.updated|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('activite_index') }}">back to list</a>

	<a href="{{ path('activite_edit', {'id': activite.id}) }}">edit</a>

	{{ include('admin/activite/_delete_form.html.twig') }}
{% endblock %}
