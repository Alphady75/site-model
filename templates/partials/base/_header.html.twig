<!-- Main Header-->
<header
	class="main-header header-style-seven">
	<!-- Header Top -->
	<div class="header-top">
		<div class="auto-container">
			<div class="inner-container">
				<div
					class="top-left">
					<!-- Info List -->
					<ul class="list-style-one light">
						<li>
							<i class="fa fa-phone"></i>
							<a href="tel:{{apropo().telephone}}">{{apropo().telephone}}</a>
						</li>
						<li>
							<i class="fa fa-envelope"></i>
							<a href="mailto:{{apropo().email}}">{{apropo().email}}</a>
						</li>
						<li>
							<i class="fa fa-map-marker-alt"></i>
							{{apropo().bureau}}</li>
					</ul>
				</div>

				<div class="top-right">
					<ul class="social-icon-one light">
						<li>
							<a href="#">
								<span class="fab fa-facebook-f"></span>
							</a>
						</li>
						<li>
							<a href="#">
								<span class="fab fa-instagram"></span>
							</a>
						</li>
						<li>
							<a href="#">
								<span class="fab fa-twitter"></span>
							</a>
						</li>
						<li>
							<a href="#">
								<span class="fab fa-linkedin"></span>
							</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</div>
	<!-- Header Top -->

	<div class="header-lower">
		<div
			class="auto-container">
			<!-- Main box -->
			<div class="main-box">
				<div class="logo">
					<a href="{{path('accueil')}}">
						<img src="{{asset('img/logo.png')}}" width="120" alt="Logo {{ appName }}" title="{{appName}}">
					</a>
				</div>

				<!--Nav Box-->
				<div class="nav-outer">
					<nav class="nav main-menu">
						<ul class="navigation">
							<li class="{{ app.request.attributes.get('_route') == 'demande_create' ? 'current' : '' }}">
								<a href="{{path('demande_create', {requete: 1})}}">Cr√©ation d'entreprise</a>
							</li>
							<li class="{{ app.request.attributes.get('_route') == 'apropos' ? 'current' : '' }}">
								<a href="{{path('apropos')}}">A propos</a>
							</li>
							<li class="{{ app.request.attributes.get('_route') == 'contact' ? 'current' : '' }}">
								<a href="{{path('contact')}}">Contact</a>
							</li>
							<li class="d-flex {{ app.request.attributes.get('_route') == 'app_login' ? 'current' : '' }}">
								{% if app.user %}
									<a href="{{path('app_redirection')}}">
										<i class="fa fa-user-circle me-2"></i>
										Espace
										{{app.user.compte}}</a>
								{% else %}
									<a href="{{path('app_login')}}">
										<i class="fa fa-user-circle me-2"></i>
										Se connecter</a>
								{% endif %}
							</li>
						</ul>
					</nav>
				</div>

				<!-- Main Menu End-->
				<div
					class="outer-box">

					<!-- Info Btn -->
					<a href="tel:{{apropo().telephone}}" class="info-btn-two">
						<i class="icon fa fa-phone"></i>
						<small>Besoin d'aide?</small>
						<strong>{{apropo().telephone}}</strong>
					</a>

					<!-- Mobile Nav toggler -->
					<div class="mobile-nav-toggler">
						<span class="icon lnr-icon-bars"></span>
					</div>
				</div>
			</div>
		</div>
	</div>

	<!-- Mobile Menu  -->
	<div class="mobile-menu">
		<div class="menu-backdrop"></div>
		<!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header-->

		<nav class="menu-box">
			<div class="upper-box">
				<div class="nav-logo">
					<a href="{{path('accueil')}}">
						<img src="{{asset('img/logo.png')}}" width="90" alt="Logo {{ appName }}" title="{{appName}}">
					</a>
				</div>
				<div class="close-btn">
					<i class="icon fa fa-times"></i>
				</div>
			</div>
			<ul
				class="navigation clearfix"><!--Keep This Empty / Menu will come through Javascript-->
			</ul>
			<ul class="contact-list-one">
				<li>
					<i class="icon lnr-icon-phone-handset"></i>
					<span class="title">Appelez maintenant</span>
					<div class="text">
						<a href="tel:{{apropo().telephone}}">{{apropo().telephone}}</a>
					</div>
				</li>
				<li>
					<i class="icon lnr-icon-envelope1"></i>
					<span class="title">Envoyer un e-mail</span>
					<div class="text">
						<a href="mailto:{{apropo().email}}">{{apropo().email}}</a>
					</div>
				</li>
				<li>
					<i class="icon lnr-icon-map-marker"></i>
					<span class="title">Addresse</span>
					<div class="text">{{apropo().bureau}}</div>
				</li>
				<!--li class="border radius pe-2">
																									<i class="fa fa-{{app.user ? 'user-circle' ? 'globe'}} ms-2"></i>
																									{% if app.user %}
																										<a href="{{path('app_login')}}">Se connecter</a>
																									{% else %}
																										<a href="{{path('app_redirection')}}">{{app.user}}</a>
																									{% endif %}
																								</li-->
			</ul>
			<ul class="social-links">
				{% for reseau in apropo().communautes %}
					<li>
						<a href="{{reseau.lien}}" target="_blank">
							<i class="fab fa-{{reseau.icon}}"></i>
						</a>
					</li>
				{% endfor %}
			</ul>
		</nav>
	</div>
	<!-- End Mobile Menu -->

	<!-- Sticky Header  -->
	<div class="sticky-header">
		<div class="auto-container">
			<div
				class="inner-container">
				<!--Logo-->
				<div class="logo">
					<a href="{{path('accueil')}}" title="">
						<img src="{{asset('img/logo.png')}}" width="90" alt="Logo {{ appName }}" title="{{appName}}">
					</a>
				</div>

				<!--Right Col-->
				<div
					class="nav-outer">
					<!-- Main Menu -->
					<nav class="main-menu">
						<div class="navbar-collapse show collapse clearfix">
							<ul
								class="navigation clearfix"><!--Keep This Empty / Menu will come through Javascript-->
							</ul>
						</div>
					</nav>
					<!-- Main Menu End-->

					<!--Mobile Navigation Toggler-->
					<div class="mobile-nav-toggler">
						<span class="icon lnr-icon-bars"></span>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- End Sticky Menu -->
</header>
<!--End Main Header -->
